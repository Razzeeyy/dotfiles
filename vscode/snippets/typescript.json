{
	"Vue Prop": {
		"prefix": "vp",
		"body": [
			"@Prop($1) $2?: $3;",
			"\t\t$0"
		],
		"description": "Vue Prop"
	},

	"Vue Prop Default": {
		"prefix": "vpd",
		"body": [
			"@Prop({ type: $1, default: $2 }) $3!: $4;",
			"\t\t$0"
		],
		"description": "Vue Prop Default"
	},

	"Vue Prop Required": {
		"prefix": "vpr",
		"body": [
			"@Prop({ type: $1, required: true }) $2!: $3;",
			"\t\t$0"
		],
		"description": "Vue Prop Required"
	}
}
